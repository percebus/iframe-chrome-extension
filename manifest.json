{
    "name": "iframe-chrome-extension",
    "description": "<iframe chrome-extension",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": ["scripting"],
    "content_scripts": [
        {
            "js": [
                "src/content/content.js"
            ],
            "css": [
                "src/content/content.css"
            ],
            "run_at": "document_idle",
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}
